VITE_API_BASE_URL=http://localhost:8000/api/v1

# Activer la fonctionnalité "Optimisation intelligente par IA (expérimental)"
# Définir à "false" pour désactiver complètement cette option (cachée dans l'interface)
# Par défaut désactivé (false) pour un déploiement simple sans fonctionnalité expérimentale
VITE_ENABLE_RL_OPTIMIZATION=false

# Activer l'authentification et la gestion de comptes
# En production sans gestion de comptes, définir à "false" pour cacher les boutons de connexion/inscription
# Par défaut désactivé (false) pour permettre un déploiement simple sans base de données
VITE_ENABLE_AUTH=false

